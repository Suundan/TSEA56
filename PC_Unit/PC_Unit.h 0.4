				else if (round(i / (j + 1)) <= 3) {
					testMap[i][j] = 'w';
				}
				else {
					testMap[i][j] = 'o';
				}
		}
		testMap[13][2] = 'g';
		testMap[11][2] = 'p';
		Drawing::Pen ^pen = gcnew Drawing::Pen(Color::DarkSlateGray, 3.0);
		for (int i = 0; i < mapRows; i++) {
			for (int j = 0; j < mapCols; j++) {
				paintMapRect(testMap[i][j], i, j);
			}
		}
	}
